{
    "sourceType": "unambiguous",
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": 100
                }
            }
        ],
        "@babel/preset-typescript",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic",
                "importSource": "@emotion/react"
            }
        ],
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        "@emotion/babel-plugin",
        "babel-plugin-macros"
    ]
}